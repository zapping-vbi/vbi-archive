# $Id: Makefile.in,v 1.3 2000-09-04 16:24:32 garetxe Exp $

OBJS	= preview.o tkme.o tkAppInit.o

all: $(OBJS)
	$(LD) -r $(OBJS) -o test.o

%.o: %.c
	$(CC) $(CFLAGS) $(DEFS) -c $< -o $@

clean:
	-rm $(OBJS) *.d *% *~ *.bak core log test.o

%.d: %.c
	@echo Creating $@
	@$(SHELL) -ec '$(CC) -MM $(CFLAGS) $< \
	| sed '\''s/\($*\)\.o[ :]*/\1.o $@ : /g'\'' > $@; \
	[ -s $@ ] || rm -f $@'

-include $(OBJS:.o=.d)

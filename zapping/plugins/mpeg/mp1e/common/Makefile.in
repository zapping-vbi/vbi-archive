# $Id: Makefile.in,v 1.1 2000-08-09 09:40:14 mschimek Exp $

OBJS	= fifo.o bstream.o alloc.o mmx.o profile.o

all: $(OBJS)
	$(LD) -r $(OBJS) -o common.o

%.o: %.c
	$(CC) $(CFLAGS) $(DEFS) -c $< -o $@

clean:
	-rm $(OBJS) *.d *% *~ *.bak core log common.o

%.d: %.c
	@echo Creating $@
	@$(SHELL) -ec '$(CC) -MM $(CFLAGS) $< \
	| sed '\''s/\($*\)\.o[ :]*/\1.o $@ : /g'\'' > $@; \
	[ -s $@ ] || rm -f $@'

-include $(OBJS:.o=.d)
